    "caption": "DB Notebook",
    "content": "SELECT user();\nUSE cookbook;\n-- Want to display specific columns and rows from a table.\nSELECT t, srcuser, srchost, dstuser, dsthost, size FROM mail;\nSELECT srcuser, srchost, t, size FROM mail;\nSELECT t, srcuser, srchost FROM mail WHERE srchost = 'venus';\nSELECT t, srcuser, srchost FROM mail WHERE srchost LIKE 's%';\nSELECT t, srcuser, srchost, dstuser, dsthost, size FROM mail WHERE srcuser = 'barb' AND dstuser = 'tricia';\nSELECT t, CONCAT(srcuser, '@', srchost), size FROM mail;\n-- You can use TABLE instead of SELECT but TABLE supports only ORDER BY and LIMIT clauses only\nTABLE mail ORDER BY size DESC LIMIT 4;\n-- Naming Query Result Columns with aliases\nSELECT DATE_FORMAT(t, '%M %e, %Y'), CONCAT(srcuser,'@',srchost), size FROM mail;\nSELECT\nDATE_FORMAT(t, '%M %e, %Y') AS date_sent,\nCONCAT(srcuser,'@',srchost) AS sender,\nsize FROM mail;\nSELECT\nDATE_FORMAT(t, '%M %e, %Y') AS 'Date of message',\nCONCAT(srcuser,'@',srchost) AS 'Message sender',\nsize AS 'Number of byts' FROM mail;\nSELECT 1 AS 'INTEGER';\nSELECT t, srcuser, dstuser, size/1024 AS Kilobytes\nFROM mail;\nSELECT t, srcuser, dstuser, size/1024 AS Kilobytes FROM mail\nWHERE size/1024 > 500;\n-- Sorting Query Result\nSELECT t, srcuser, srchost, dstuser, dsthost, size FROM mail WHERE dstuser = 'tricia'\nORDER BY srchost, srcuser;\nSELECT t, srcuser, srchost,dstuser, dsthost, size FROM mail\nWHERE size > 50000 ORDER BY size DESC;\n-- Removing Duplicate Rows\nSELECT srcuser from mail;\n-- Using DISTINCT to remove duplicate\nSELECT DISTINCT srcuser from mail;\n-- Using COUNT to find no of result\nSELECT COUNT(srcuser) FROM mail;\nSELECT COUNT(DISTINCT srcuser) FROM mail;\nSELECT DISTINCT YEAR(t), MONTH(t), DAYOFMONTH(t) FROM mail;\n-- Working with NULL VALUES\n-- Failing Statement\nSELECT * FROM expt WHERE score = NULL;\nSELECT * FROM expt WHERE score <> NULL;\n-- MySQL NULL operator IS NULL, IS NOT NULL, <=>\nSELECT * FROM expt WHERE score IS NULL;\nSELECT * FROM expt WHERE score IS NOT NULL;\nSELECT * FROM expt WHERE score <=> NULL;\nSELECT NULL = NULL, NULL <=> NULL;\n-- Using IF() to replace NULL value into different value\n-- IF SYNTAX: IF(condition, true, false)\nSELECT subject, test, if (score IS NULL, 'Unknown', score) AS 'score' FROM expt;\n-- Using IFNULL(IF NOT NULL[false], IF NULL[true])\nSELECT subject, test, IFNULL(score, 'Unknown') AS 'score' FROM expt;\n-- The above is equal to IF (score IS NOT NULL, score, 'Unknown')\n-- Using COALESCE\nSELECT subject, test, COALESCE(score, 'Unknown') AS 'score' FROM expt;\n-- Using VIEWS to simplify table access\nSELECT\nDATE_FORMAT(t, '%M %e, %Y') AS 'date_sent',\nCONCAT(srcuser,'@',srchost) AS 'sender',\nCONCAT(dstuser,'@',dsthost) AS 'recipient',\nsize FROM mail;\nCREATE VIEW mail_view AS\nSELECT\nDATE_FORMAT(t, '%M %e, %Y') AS 'date_sent',\nCONCAT(srcuser,'@',srchost) AS 'sender',\nCONCAT(dstuser,'@',dsthost) AS 'recipient',\nsize FROM mail;\nSELECT date_sent, sender, size FROM mail_view\nWHERE size > 100000 ORDER BY size;\n-- Using JOIN to query the result from two or More tables\nSELECT profile.id, name, service, contact_name \nFROM profile_contact INNER JOIN profile ON profile.id = profile_id;\n-- Using Subqueries to join two or more tables\nSELECT profile_id, service, contact_name FROM profile_contact\nWHERE profile_id = (SELECT id from profile WHERE name = 'Nancy');\n-- Using LIMIT with SELECT\nSELECT * FROM profile LIMIT 1;\nSELECT * FROM profile LIMIT 3;\nSELECT * FROM profile ORDER BY birth;\nSELECT * FROM profile ORDER BY birth LIMIT 1;\nSELECT * FROM profile ORDER BY birth DESC LIMIT 1;\nSELECT name, DATE_FORMAT(birth, '%m-%d') AS 'birthday' FROM profile ORDER BY birthday LIMIT 1;\nSELECT * FROM profile ORDER BY birth LIMIT 2,1;\nSELECT * FROM profile ORDER BY birth DESC LIMIT 2,1;\n-- Using COUNT to find out no of record in a table\nSELECT COUNT(*) FROM profile;\n-- Find the 4 people who born recently in ascending order\nSELECT * from (SELECT name, birth FROM profile ORDER BY birth DESC LIMIT 4) AS t ORDER BY birth;\n-- Combining two are more SELECT result by using UNION and UNION ALL\n-- One condition is no of columns must be same\nSELECT DISTINCT srcuser as user, srchost as host from mail\nUNION\nSELECT DISTINCT dstuser as user, dsthost as host from mail;\n-- Let's find 4 users who send highest no of emails, and 4 users who received highest no of emails\n(SELECT CONCAT(srcuser,'@',srchost) AS user, COUNT(*) AS emails FROM mail GROUP BY srcuser, srchost ORDER BY emails DESC LIMIT 4)\nUNION ALL\n(SELECT CONCAT(dstuser,'@',dsthost) AS user, COUNT(*) AS emails FROM mail GROUP BY dstuser, dsthost ORDER BY emails DESC LIMIT 4)\norder by user;\nSELECT CONCAT(srcuser,'@',srchost) AS user, COUNT(*) AS emails FROM mail GROUP BY srcuser, srchost ORDER BY emails DESC LIMIT 4;\nSELECT CONCAT(dstuser,'@',dsthost) AS user, COUNT(*) AS emails FROM mail GROUP BY dstuser, dsthost ORDER BY emails DESC LIMIT 4;\n(SELECT CONCAT(srcuser,'@',srchost) AS user, COUNT(*) AS emails FROM mail GROUP BY srcuser, srchost ORDER BY emails DESC LIMIT 4)\nUNION\n(SELECT CONCAT(dstuser,'@',dsthost) AS user, COUNT(*) AS emails FROM mail GROUP BY dstuser, dsthost ORDER BY emails DESC LIMIT 4)\norder by user;\n-- UNION by default remove the duplicate the same result!\n-- find user send and receive mail count from mail TABLE\nSELECT CONCAT(srcuser,'@',srchost) AS user, \nCOUNT(*) AS send, \n(SELECT COUNT(*) FROM mail d WHERE d.dstuser = m.srcuser AND d.dsthost = m.srchost) AS received \nFROM mail m \nGROUP BY srcuser, srchost\nORDER BY send DESC;\n ",
